{
	"name": "gh-user-cli",
	"version": "1.0.0",
	"description": "Open the GitHub or NPM profile of the given or current user.",
	"keywords": [
		"cli",
		"cli-app",
		"command-line",
		"user",
		"username",
		"profile",
		"github",
		"gh",
		"npm",
		"open",
		"current"
	],
	"license": "MIT",
	"repository": "tommy-mitchell/gh-user-cli",
	"author": {
		"name": "Tommy Mitchell",
		"url": "https://tommymitchell.io"
	},
	"type": "module",
	"bin": {
		"gh-user": "./dist/cli.js"
	},
	"files": [
		"dist"
	],
	"engines": {
		"node": "^18.18 || ^20.8 || ^21"
	},
	"scripts": {
		"prepare": "npm run build",
		"build": "tsc -p tsconfig.build.json && execify --all && dprint fmt --excludes '!dist'",
		"lint": "xo",
		"format": "dprint fmt && xo --fix",
		"test": "c8 ava"
	},
	"ava": {
		"extensions": {
			"ts": "module"
		},
		"nodeArguments": [
			"--import=tsimp",
			"--import=esmock"
		]
	},
	"dependencies": {
		"execa": "^9.3.0",
		"meow": "^13.2.0",
		"open": "^10.1.0"
	},
	"devDependencies": {
		"@tommy-mitchell/dprint-config": "^0.2.0",
		"@tommy-mitchell/eslint-config-xo": "^0.3.1",
		"@tommy-mitchell/tsconfig": "^2.1.0",
		"@types/node": "^18.18",
		"ava": "^6.1.3",
		"c8": "^10.1.2",
		"dprint": "^0.47.2",
		"esmock": "^2.6.6",
		"execify-cli": "beta",
		"get-executable-bin-path": "^0.1.0",
		"tsimp": "^2.0.11",
		"type-fest": "^4.21.0",
		"typescript": "~5.5.3",
		"xo": "^0.58.0"
	}
}
